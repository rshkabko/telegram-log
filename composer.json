    "description": "Send laravel logs into Telegram Chat via bot",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "illuminate/support": ">=8",
        "monolog/monolog": ">=2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4" : {
            "Flamix\\TelegramLog\\": "src"
        }
    },
    "authors": [
        {
            "name": "Yanuar",
            "email": "yanuaraditia@outlook.com"
        },
        {
            "name": "Roman Shkabko",
            "email": "r.shkabko@flamix.eamil"
        }
    ],
    "prefer-stable": false,
    "extra": {
        "laravel": {
            "providers": [
                "Flamix\\TelegramLog\\TelegramServiceProvider"
            ]
        }
    }
}
