{
    "name": "flamix/telegram-log",
    "description": "Send laravel logs into Telegram Chat via bot",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "illuminate/support": ">=8",
        "monolog/monolog": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4" : {
            "TelegramLog\\" : "src"
        }
    },
    "authors": [
        {
            "name": "Yanuar",
            "email": "yanuaraditia@outlook.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "TelegramLog\\TelegramServiceProvider"
            ]
        }
    }
}
